{
    "entrypoint": "index.html",
    "shell": "index.js",
    "sources": [
        "Assets/**/*",
        "components/**/*",
        "pages/**/*",
        "style.css",
        "package.json"
    ],
    "extraDependencies": [
        "CNAME",
        "node_modules/@fortawesome/fontawesome-free/**"
    ],
    "componentDir": "components",
    "moduleResolution": "node",
    "npm": true,
    "builds": [
        {
            "name": "public",
            "addServiceWorker": false,
            "js": {
                "minify": true,
                "compile": false
            },
            "css": {
                "minify": true
            },
            "html": {
                "minify": true
            },
            "bundle": false,
            "addPushManifest": false
        }
    ]
}