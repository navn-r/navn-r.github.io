  "author": "Navinn Ravindaran",
  "description": "Portfolio Website",
  "license": "Unlicense",
  "homepage": "https://github.com/navn-r/navn-r.github.io#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navn-r/navn-r.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/navn-r/navn-r.github.io/issues"
  },
  "engines": {
    "node": ">=12.x"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "svelte-check --tsconfig tsconfig.json  --fail-on-warnings --fail-on-hints",
    "format": "prettier --write src",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "husky": "^7.0.4",
    "lint-staged": "^14.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.54.9",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.8.3",
    "vite": "^2.9.16"
  },
  "lint-staged": {
    "src/*": [
      "npm run lint",
      "npm run format"
    ]
  }
}
